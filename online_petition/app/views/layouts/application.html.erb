<!DOCTYPE html>
<html>
<head>
  <title>OnlinePetition</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="col-md-12 well sidebar-nav header">
  <%= render partial: "header" %>
</div>

<div class="col-md-12">
  <div class="col-md-9">
    <%= yield %>
  </div>

  <div class="col-md-3">
    <%= render partial: "links_menu" %>
  </div>
</div>
<div class="col-md-12 footer">
  <%= render partial: "footer" %>
</div>
</body>
</html>
